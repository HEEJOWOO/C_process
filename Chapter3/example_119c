//구조체 사용하기
#include<stdio.h>
#include<string.h>

struct tag {
	char name[20];
	char number[20];
	char address[50];
};

void main(void) {
	struct tag ad;
	strcpy_s(ad.name,20 ,"우희조");
	strcpy_s(ad.number, 20, "010-0000-0000");
	strcpy_s(ad.address, 50, "서울 특별시 -");

	puts(ad.name);
	puts(ad.number);
	puts(ad.address);
	
}
